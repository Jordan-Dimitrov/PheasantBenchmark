<Window x:Class="PheasantBench.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PheasantBench.App"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label x:Name="ProcessorName" Margin="0,0,0,10">Processor Name:</Label>
        <Label x:Name="CPUCores" Margin="0,0,0,10">CPU cores:</Label>
        <Label x:Name="Architecture" Margin="0,0,0,10">Architecture:</Label>
        <Label x:Name="MachineName" Margin="0,0,0,10">Machine Name:</Label>
        <Label x:Name="OSVersion" Margin="0,0,0,10">OS Version:</Label>
        <Button x:Name="StartBenchmarkButton" Click="StartBenchmarkButton_Click">Start Benchmark</Button>
        <StackPanel>
            <Label Margin="0,0,0,0">Score:</Label>
            <Label x:Name="BenchmarkScore" Margin="0,0,0,0"/>
        </StackPanel>
    </StackPanel>
</Window>